mixin storageFeature(icon, title, text)
	+b.storage-feature
		+icon(icon)
		.title.fw-500=title
		p= text

mixin distributorItem(title,address,coords)
	+b.coord-item
		+e.title
			| #{title}
		+e.item-text
			+e.info-address.small #[span Адрес]: #{address}
			+e.info-address.small #[span GPS координаты:] #{coords}

