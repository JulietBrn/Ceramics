									| 19 008/ 48 под.

				+b.item-top.bg-wrap
					+img(`img/storage-bg-3.png`, 'bg').picture-bg
					+e.item-wrap
						+e.item-row.row
							.col-12.col-md-5.order-md-2
								.icons-wrap
									+img(src=`img/konigstein-2.svg`, alt="").brand
									+img(src=`img/master-ceramic.svg`, alt="").brand
							.col-12.col-md-7
								.h5 Кирпич керамический лицевой пустотелый Kôningstein/ Мастер Керамик
				.table-wrap
					table
						+tHead
						tbody
							tr
								td КР-кл-пу 1 НФ
								td(data-title=sizeDescription)
									.dash
									| 250х120х65
								td(data-title=quantityInPalletDescription)
									.dash
									| 300
								td(data-title=quantityInTruckDescription)
									.dash
									| 5 100/ 17 под.
								td(data-title=quantityInRailcarDescription)
